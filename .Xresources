Xcursor.size: 16W
URxvt.urgentOnBell: true
URxvt.scrollBar: false
URxvt.font: xft:hack:size=14,xft:FreeSans,xft:FontAwesome,xft:devicons-regular
URxvt.depth: 24
URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1
URxvt.perl-ext-common: default,matcher,vtwheel

! ---------------------------------------------------------------------
! ROFI Color theme
! ---------------------------------------------------------------------
rofi.color-enabled: true
rofi.color-window: #882f343f, #002b36, #2f343f
rofi.color-normal: #2f343f, #f3f4f5, #393f4d, #82a9b5, #2f343f
rofi.color-active: #2f343f, #268bd2, #2f343f, #268bd2, #f3f4f5
rofi.color-urgent: #2f343f, #dc322f, #2f343f, #dc322f, #f3f4f5
rofi.separator-style: none
rofi.opacity: 50
rofi.hide-scrollbar: True
